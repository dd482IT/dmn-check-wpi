package de.redsix.dmncheck.validators;

import org.camunda.bpm.model.dmn.instance.KnowledgeSource;

@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.regex.RegexChecker")
public class KnowledgeSourceIdAndNameValidator extends IdAndNameValidator<KnowledgeSource> {

    @org.checkerframework.dataflow.qual.Pure
    public @org.checkerframework.checker.regex.qual.Regex String getName( KnowledgeSourceIdAndNameValidator this) {
        return "knowledge source";
    }

    @org.checkerframework.dataflow.qual.Pure
    public  Class<KnowledgeSource> getClassUnderValidation( KnowledgeSourceIdAndNameValidator this) {
        return KnowledgeSource.class;
    }
}
